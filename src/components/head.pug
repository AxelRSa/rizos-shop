mixin head(props)
	doctype html
	html(lang="en")
		head
			meta(charset="UTF-8")
			meta(http-equiv="X-UA-Compatible", content="IE=edge")
			meta(name="viewport", content="width=device-width, initial-scale=1.0")
			meta(name="description" content=props.description)
			link(rel="shortcut icon" href=props.icon)
			title=props.title
		body 
			block